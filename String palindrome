 package org.interview;

public class PalindromeString {
	public static void main(String[] args) {
		String s = "venkat";
		String rev = "";
		for(int i = s.length()-1;i>0;i--) {
			Character c = s.charAt(i);
			rev = rev+c;
			}
		if(s.equals(rev)) {
			System.out.println(" Palindrome");
		}
		else {
			System.out.println(" not a Palindrome");
		}
	}


}
